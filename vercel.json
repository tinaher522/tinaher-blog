{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=0, no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "^/api/?$",
      "dest": "https://api.example.com",
      "headers": {
        "Content-Type": "application/json",
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "redirects": [
    {
      "source": "/resume",
      "destination": "/about",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/posts/(.*)",
      "destination": "/:1"
    }
  ],
  "trailingSlash": false
}

